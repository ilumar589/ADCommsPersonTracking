{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ObjectDetection": {
    "ModelPath": "models/yolo11n.onnx",
    "ConfidenceThreshold": 0.45,
    "IouThreshold": 0.5
  },
  "AccessoryDetection": {
    "ModelPath": "",
    "ConfidenceThreshold": 0.5
  },
  "Processing": {
    "MaxDegreeOfParallelism": 3
  },
  "ImageAnnotation": {
    "BoxColor": "#00FF00",
    "BoxThickness": 2,
    "ShowLabels": true,
    "FontSize": 12
  },
  "VideoProcessing": {
    "MaxFrames": 100,
    "FrameInterval": 1
  },
  "BlobStorage": {
    "ContainerName": "video-frames"
  }
}
