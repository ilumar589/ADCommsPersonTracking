{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ObjectDetection": {
    "Mode": "auto",
    "ModelPath": "models/yolo11n.onnx",
    "ConfidenceThreshold": 0.45,
    "IouThreshold": 0.5,
    "HttpRetryIntervalMinutes": 5
  },
  "Yolo11": {
    "Endpoint": "http://localhost:8000"
  },
  "ImageAnnotation": {
    "BoxColor": "#00FF00",
    "BoxThickness": 2,
    "ShowLabels": true,
    "FontSize": 12
  }
}
